"weekGT":"<h1>Week at a Glance</h1>
<p>Some desciption I may write later</p>
<hr>
<h3>Import OrgSync</h3>
<p>
  <form action=\"/weekGT.html\" method=\"POST\" class=\"form-inline\">
    <div class=\"form-group\">
      <label for=\"sel1\">Select Week:</label>
      <select class=\"form-control\" id=\"sel1\" name=weekSel>
        <option>This Week</option>
        <option selected=\"selected\">Next Week</option>
        <option>2 Weeks Ahead</option>
        <option>3 Weeks Ahead</option>
      </select>
    </div>
    <button type=\"submit\" class=\"btn btn-default\" name=\"save\" value=\"save\">Import</button>
  </form>
</p>
<hr>
<p>{accordion}</p>
<img class=\"img-responsive\" src=\"/images/{image}\" alt=\"/images/template.png\">"
}
